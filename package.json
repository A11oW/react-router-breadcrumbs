{
  "name": "react-router-breadcrumbs",
  "version": "1.0.8",
  "description": "Breadcrumb component for react-router",
  "keywords": [
    "react",
    "react-router",
    "react-component",
    "breadcrumb",
    "path"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nutgaard/react-router-breadcrumbs"
  },
  "bugs": {
    "url": "https://github.com/nutgaard/react-router-breadcrumbs/issues"
  },
  "homepage": "https://github.com/nutgaard/react-router-breadcrumbs",
  "author": "Nicklas Utgaard",
  "license": "X11",
  "scripts": {
    "app": "browserify index.js -o bundle.js -t [ babelify ] -x react -x react-dom -x react-router",
    "vendor": "browserify -r react -r react-dom -r react-router > vendor.js",
    "minify": "uglifyjs bundle.js --compress --mangle --screw-ie8 --output bundle.min.js && uglifyjs vendor.js --compress --mangle --screw-ie8 --output vendor.min.js",
    "build": "rm -f {vendor,bundle}{.min,}.js && npm run app && npm run vendor && npm run minify"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "uglify-js": "^2.6.2"
  },
  "dependencies": {
    "react-router-breadcrumbs": "*",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.1",
    "react": "^0.14.7"
  }
}
